{
  "name": "genesis-docs",
  "version": "1.0.0",
  "description": "Help page for Genesis",
  "scripts": {
    "test": "mocha mochaspec.js",
    "postinstall": "npm install -g gulp",
    "start": "npx gulp && node app.js",
    "restart": "pm2 kill && pm2 start pm2.json",
    "lint": "npx eslint gulpfile.js routes/. assets/js/main.js mochaspec.js",
    "lint:fix": "npx eslint gulpfile.js routes/. assets/js/main.js mochaspec.js --fix",
    "dev": "npx nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WFCD/genesis-docs.git"
  },
  "keywords": [
    "warframe",
    "genesis"
  ],
  "author": "TobiTenno",
  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/WFCD/genesis-docs/issues"
  },
  "homepage": "https://genesis.warframestat.us",
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "serve-favicon": "~2.4.5",
    "supertest": "^3.4.2",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "debug": "^2.6.9",
    "del": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-minify": "^3.1.0",
    "mocha": "^6.2.2",
    "morgan": "^1.9.1",
    "sinon": "^8.0.1"
  },
  "nodemonConfig": {
    "ignore": [],
    "watch": [
      "assets",
      "public"
    ],
    "env": {
      "LOG_LEVEL": "debug",
      "NODE_ENV": "production",
      "PORT": 3647
    }
  }
}

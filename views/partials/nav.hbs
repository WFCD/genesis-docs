<div class="navbar navbar-default navbar-fixed-top">
	<div class="container">
		<div class="navbar-header">
			<a name="Home" href="/" class="navbar-brand"><img src="https://i.imgur.com/g19AKqV.png" height="auto" style="margin-top: -5px; display:inline;" />Genesis</a>
			<button name="Navigation Bar" class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target="#navbar-main" aria-expanded="false">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
		</div>
		<div class="navbar-collapse collapse" id="navbar-main" aria-expanded="false" style="height: 1px;">
			<ul class="nav navbar-nav">
				<li class="dropdown">
					<a name="Projects Navigation" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Projects <span class="caret"></span></a>
					<ul class="dropdown-menu" role="menu">
						<li><a name="Drops" rel="noopener" target="_blank" href="https://drops.warframestat.us">Drops</a></li>
						<li><a name="Genesis" rel="noopener" target="_blank" href="https://genesis.warframestat.us">Genesis</a></li>
						<li><a name="API Documentation" rel="noopener" target="_blank" href="https://docs.warframestat.us">API Docs</a></li>
						<li><a name="Status" rel="noopener" target="_blank" href="https://warframestat.us">Status</a></li>
						<li><a name="Warframe Hub" rel="noopener" target="_blank" href="https://hub.warframestat.us">Hub</a></li>
					</ul>
				</li>
				<li class="dropdown theme-picker">
					<a name="Theme Selection" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Theme <span class="caret"></span></a>
					<ul class="dropdown-menu" role="menu">
						<li class="text-center" id="theme_night">
							<a name="Nightmode" href="#" id="nightmode">
								<img class="cc-img inv" src="img/themes/night/icon.png">
								<span>Night Mode</span>
							</a>
						</li>
						<li class="text-center" id="theme_retro">
							<a name="Retro" href="#" id="retro">
								<img class="cc-img inv" src="img/themes/retro/icon.png">
								<span>Retro Mode</span>
							</a>
						</li>
						<li class="text-center" id="theme_eidolon">
							<a name="Eidolon" href="#" id="eidolon">
								<img class="cc-img inv" src="img/themes/eidolon/icon.png">
								<span>Eidolon</span>
							</a>
						</li>
						<li class="text-center" id="theme_none">
							<a name="No Theme" href="#" id="no-theme">
								<span>No Theme</span>
							</a>
						</li>
					</ul>
				</li>
			</ul>
			<ul class="nav navbar-nav navbar-right" id="navbar-right">
				<li class="dropdown" id="jump-nav">
					<a name="Commands Navigation" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Jump to... <span class="caret"></span></a>
					<ul class="dropdown-menu" role="menu">
						{{> nav-list}}
					</ul>
				</li>
				{{#if loggedIn}}
					<li class="nav-item dropdown">
						<a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">{{user.username}}</a>
						<li class="dropdown">
							<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Servers <span class="caret"></span></a>
							<ul class="dropdown-menu" role="menu">
								{{#each user.guilds}}
									<li><a name="Drops" rel="noopener" href="/server/{{this.id}}">{{this.name}}</a></li>
								{{else}}
									<li><a name="Drops" rel="noopener">No Servers</a></li>
								{{/each}}
							</ul>
						</li>
					</li>
				{{else}}
					<li>
						<button type="button" class="btn btn-outline-default navbar-btn" onclick="javascript:location.href='/login';">Log in</button>
					</li>
				{{/if}}
			</ul>
		</div>
	</div>
</div>
